{
  "name": "@janus-validator/domain",
  "version": "0.8.1",
  "description": "Domain classes for validation and generation - defines the set of valid values for validators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@janus-validator/regex-parser": "0.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage": "jest --coverage --coverageReporters=text-summary",
    "test:pbt:ci": "JANUS_PBT_PROFILE=ci jest",
    "test:pbt:complete": "JANUS_PBT_PROFILE=complete jest",
    "test:pbt:short": "JANUS_PBT_PROFILE=ci jest",
    "test:pbt:long": "JANUS_PBT_PROFILE=complete jest"
  },
  "keywords": [
    "validator",
    "domain",
    "typescript",
    "schema",
    "type-checking"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "fast-check": "^3.23.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
