{
  "name": "@janus-validator/core",
  "version": "0.8.0",
  "description": "A combinator library for defining validators that validate objects and provide examples",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib": {
      "types": "./dist/com/techlloyd/janus/lib/index.d.ts",
      "default": "./dist/com/techlloyd/janus/lib/index.js"
    },
    "./combinators": {
      "types": "./dist/com/techlloyd/janus/combinators/index.d.ts",
      "default": "./dist/com/techlloyd/janus/combinators/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "validator",
    "validation",
    "combinator",
    "typescript",
    "schema",
    "type-checking",
    "generator",
    "test-data"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
